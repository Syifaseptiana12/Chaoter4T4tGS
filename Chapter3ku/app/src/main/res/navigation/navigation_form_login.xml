<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_form_login"
    app:startDestination="@id/formLoginFragment">

    <fragment
        android:id="@+id/formLoginFragment"
        android:name="com.syfsdi.chapter3ku.log.FragmentLogin"
        android:label="fragment_form_login"
        tools:layout="@layout/fragmentlogin" >
        <action
            android:id="@+id/action_formLoginFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_formLoginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_formLoginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_formLoginFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_formLoginFragment_to_lupasFragment"
            app:destination="@id/lupasFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.syfsdi.chapter3ku.log.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/chat_fragment" />
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.syfsdi.chapter3ku.log.FragmentHelp"
        android:label="fragment_help"
        tools:layout="@layout/help_fragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.syfsdi.chapter3ku.log.FragmentHome"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_profileFragment2"
            app:destination="@id/profileFragment2" />
    </fragment>
    <fragment
        android:id="@+id/lupasFragment"
        android:name="com.syfsdi.chapter3ku.log.FragmentLupaPasswors"
        android:label="fragment_lupas"
        tools:layout="@layout/lupapassword_fragment" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.syfsdi.chapter3ku.log.FragmentRegister"
        android:label="fragment_register"
        tools:layout="@layout/regis_fragment" >
        <action
            android:id="@+id/action_registerFragment_to_outputHitungTahun2"
            app:destination="@id/outputHitungTahun2" />
        <action
            android:id="@+id/action_registerFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/outputHitungTahun2"
        android:name="com.syfsdi.chapter3ku.log.MenghitungTahun"
        android:label="fragment_output_hitung_tahun"
        tools:layout="@layout/menghitungtahun_fragment" />
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.syfsdi.chapter3ku.log.FragmentProfile"
        android:label="fragment_profile"
        tools:layout="@layout/profile_fragment" />
</navigation>